	<gate:resource id="defaultTextTokenizer"
				   resource-class="hitex.gate.Tokenizer">
		<gate:parameters>
			<entry key="rulesURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/resources/tokenizer/rules.txt
				</value>
			</entry>
		</gate:parameters>
	</gate:resource>

	<gate:resource id="defaultSentenceSplitter"
				   resource-class="hitex.gate.SentenceSplitter">
	</gate:resource>

	<gate:resource id="defaultRegexConceptFinder"
				   scope="prototype"
				   resource-class="hitex.gate.regex.ConceptFinder">
		<gate:parameters>
			<entry key="useSections">
				<value>true</value>
			</entry>
			<entry key="filterURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/resources/regex/section_filter.txt
				</value>
			</entry>
			<entry key="rulesURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/resources/regex/rules.xml
				</value>
			</entry>
		</gate:parameters>
	</gate:resource>

	<gate:resource id="defaultPOSTagger"
				   resource-class="gate.creole.POSTagger">
		<gate:parameters>
			<entry key="outputAnnotationType">
				<value>Token</value>
			</entry>
			<entry key="baseSentenceAnnotationType">
				<value>Sentence</value>
			</entry>
			<entry key="baseTokenAnnotationType">
				<value>Token</value>
			</entry>
			<entry key="rulesURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/plugins/ANNIE/resources/heptag/ruleset
				</value>
			</entry>
			<entry key="lexiconURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/plugins/ANNIE/resources/heptag/lexicon
				</value>
			</entry>
		</gate:parameters>
	</gate:resource>
	<gate:resource id="defaultANNIETransducer"
				   resource-class="gate.creole.ANNIETransducer">
		<gate:parameters>
			<entry key="grammarURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/plugins/ANNIE/resources/NE/main.jape
				</value>
			</entry>
		</gate:parameters>
	</gate:resource>
	<gate:resource id="defaultOrthoMatcher"
				   resource-class="gate.creole.orthomatcher.OrthoMatcher">
		<gate:parameters>
			<entry key="definitionFileURL">
				<value type="org.springframework.core.io.Resource">
					WEB-INF/plugins/ANNIE/resources/othomatcher/listsNM.def
				</value>
			</entry>
			<entry key="minimumNicknameLikelihood">
				<value>0.5</value>
			</entry>
			<entry key="extLists">
				<value>true</value>
			</entry>
			<entry key="caseSensitive">
				<value>falshitex.gatee</value>
			</entry>
			<entry key="highPrecisionOrgs">
				<value>false</value>
			</entry>
			<entry key="personType">
				<value>Person</value>
			</entry>
			<entry key="processUnknown">
				<value>true</value>
			</entry>
			<entry key="organizationType">
				<value>Organization</value>
			</entry>
		</gate:parameters>
	</gate:resource>


	<gate:resource id="defaultSectionizer"
				   scope="prototype"
				   resource-class="hitex.gate.Sectionizer"
				   resource-name="exampleSectionizer">
		<gate:parameters>
			<entry key="headersURL">
				<value type="org.springframework.core.io.Resource">WEB-INF/resources/sectionizer/dis_headers.xml
				</value>
			</entry>
		</gate:parameters>
	</gate:resource>
