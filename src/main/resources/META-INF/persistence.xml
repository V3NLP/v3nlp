<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="1.0">

  <persistence-unit name="v3nlpPersistenceUnit" transaction-type="RESOURCE_LOCAL">
       <provider>org.hibernate.ejb.HibernatePersistence</provider>
		<properties>
            <property name="hibernate.show_sql" value="false" />
	    </properties>
        <class>gov.va.vinci.v3nlp.model.BatchJobStatus</class>
        <class>gov.va.vinci.v3nlp.expressionlib.Expression</class>
        <class>gov.va.vinci.v3nlp.registry.NlpAnnotation</class>
        <class>gov.va.vinci.v3nlp.registry.NlpAnnotationAttribute</class>
        <class>gov.va.vinci.v3nlp.registry.NlpComponentCategory</class>
        <class>gov.va.vinci.v3nlp.registry.NlpComponent</class>
        <class>gov.va.vinci.v3nlp.registry.NlpComponentProvides</class>
        <class>gov.va.vinci.v3nlp.registry.NlpComponentRequires</class>
        <class>gov.va.vinci.v3nlp.services.database.V3nlpDBRepository</class>
  </persistence-unit>
</persistence>
